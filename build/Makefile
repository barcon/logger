# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  logger_config = debug

else ifeq ($(config),release)
  logger_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := logger

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

logger:
ifneq (,$(logger_config))
	@echo "==== Building logger ($(logger_config)) ===="
	@${MAKE} --no-print-directory -C . -f logger.make config=$(logger_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f logger.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   logger"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"